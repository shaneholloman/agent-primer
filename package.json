{
	"name": "agent-primer",
	"version": "0.1.0",
	"description": "Prime your Agent sessions with preloaded skills from ~/.claude/skills and ./.claude/skills",
	"main": "dist/index.js",
	"type": "module",
	"files": [
		"dist"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/shaneholloman/agent-primer.git"
	},
	"keywords": [
		"claude",
		"claude-code",
		"skills",
		"ai",
		"cli"
	],
	"author": "Shane Holloman",
	"license": "MIT",
	"bin": {
		"agent-primer": "dist/index.js",
		"ap": "dist/index.js",
		"apx": "dist/apx.js"
	},
	"scripts": {
		"start": "bun run index.ts",
		"build": "bun build index.ts --outdir dist --target node --format esm && bun build apx.ts --outdir dist --target node --format esm && sed -i '' 's|#!/usr/bin/env bun|#!/usr/bin/env node|' dist/index.js dist/apx.js",
		"link": "bun link",
		"lint": "biome lint .",
		"format": "biome format --write .",
		"check": "biome check --write ."
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.14",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^13.0.1",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^12.0.5",
		"@semantic-release/npm": "^13.1.4",
		"@semantic-release/release-notes-generator": "^14.1.0",
		"@types/bun": "latest",
		"semantic-release": "^25.0.3"
	},
	"peerDependencies": {
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"@clack/prompts": "^1.0.0",
		"gray-matter": "^4.0.3",
		"pino": "^10.3.0",
		"zod": "^4.3.6"
	}
}
